/* pages/myCenter/myCenter.wxss */
page
{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.header_box
{
  width: 750rpx;
  height: 245rpx;
  position: relative;
}
.header_box
{
  padding-left:35rpx;
  box-sizing: border-box; 
}
.photo
{
  border-radius: 50%;
  overflow: hidden;
}
.jf
{
  color:#fefefe;
  font-size:28rpx;
  line-height: 50rpx;
  height: 50rpx;
  border-top: 2rpx solid #8cb4e5;
}
.nav_item
{
  width: 750rpx;
  height: 90rpx;
  background-color: white;
  box-sizing: border-box;
  padding: 0 36rpx;
}
.nav_name
{
  color:#4c4c4c;
  font-size: 26rpx;
  padding-left: 30rpx;
}
.jf_num
{
  color:#4c4c4c;
  font-size: 26rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.bottom_border
{
  border:2rpx solid #f6f6f6
}