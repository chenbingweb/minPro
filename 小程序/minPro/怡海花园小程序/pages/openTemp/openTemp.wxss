/* pages/openTemp/openTemp.wxss */
page
{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.item
{
  background-color: white;
  border-bottom: 2rpx solid #ececec;
  box-sizing: border-box;
  padding: 0 36rpx;
  color:#333333;
  font-size: 28rpx;
}
.js_text_area
{
  height:80rpx;
  margin-bottom: 20rpx;
}
.address
{
  align-items: flex-start;
  padding-top: 20rpx;
}
.submit
{
  width: 678rpx;
  height: 90rpx;
  margin: 0 auto;
  background: #5293e1;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
  color:#ffffff;
  border-radius: 10rpx;
  margin-top: 40rpx;
  margin-bottom: 35rpx;
}
.tip
{
  color:#aac8ec;
  font-size:26rpx;
  text-align: center;
  opacity:1;
  line-height: 130rpx;
}