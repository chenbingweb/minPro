/* pages/help/help.wxss */
page
{
  background-color: #f5f5f5;
}
.tab
{
  width: 750rpx;
  height: 84rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #eaeaea;
  margin-bottom: 16rpx;
}
.f_30_666
{
  font-size: 30rpx;
  color:#666;
  line-height: 84rpx;
  margin: 0 24rpx;
  position: relative;
}
.active
{
  color:#08d639;
}
.active::after
{
  content: '';
  position: absolute;
  width: 100%;
  height: 4rpx;
  background-color: #08d639;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
  
}
.tran
{
  transition: 0.2s all linear;
   
}
.s_box
{
  background-color: #fff;
  
}
.s_item
{
  width: 750rpx;
  height: 90rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #eaeaea;
}
.f_32_333
{
  font-size: 32rpx;
  color:333;
}
.content
{
  width: 750rpx;
  box-sizing: border-box;
  overflow: hidden;
  transition: 0.2s all linear;
  padding: 0 83rpx;
}
