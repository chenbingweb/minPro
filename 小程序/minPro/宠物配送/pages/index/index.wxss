/* pages/index/index.wxss */
/*引入模板样式*/
/* @import '../../temp/userheader.wxss';*/
@import '../../temp/temp.wxss'; 
page
{
  display: table;
  padding-bottom: 100rpx; 
  background-color: #fff;
}
.index_page
{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 250rpx;
}
.list_item
{
  width: 750rpx;
  margin: 0 auto;
  /* height: 340rpx; */

}
.list_item_title
{
  height: 80rpx;
  width: 750rpx;
  background-color: #f7f7f7;
  box-sizing: border-box;
  padding: 0 25rpx;
}
.list_status
{
  font-size: 26rpx;

}
.list_status_color_wait
{
  color:#63a6f9
}
.list_status_color_sending
{
  color:#fdde1a
}
.list_status_color_complete
{
  color:#fe736c;
}
.address_info
{
  width: 750rpx;
  box-sizing: border-box;
  padding: 0 25rpx;
  height: 260rpx;

}
.m_line
{
  width: 2rpx;
  height: 70rpx;
  background-color: #e5e5e5;
  margin-bottom: 10rpx;
}
.info_left
{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.info_right
{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
    margin-left: 30rpx;
}
.child
{
  height: 126rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items:flex-start;
  width: 630rpx;
}
.child_1
{
  height: 126rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items:flex-start;
  border-bottom: 2rpx solid #e7e7e7;
  width: 630rpx;
}
.refresh
{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 150rpx;
  width: 210rpx;
  height: 70rpx;
  border:2rpx solid #aaaeba;
  border-radius: 10rpx;
}