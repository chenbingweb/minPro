<view class="fixed">
    <form>
    <view class="absolute icon_box">
      <image src="../../images/login_bg.jpg" style="width:100%;height:100vh"></image>
    </view>
    <view class="logo_img ion_box">
      <image src="../../images/logo.png" style="width:222rpx;height:207rpx"></image>
    </view>
    <view class="account item_input flex_start">
        <input class="f_26_999 input" placeholder='请输入账号' bindinput="onAccount"></input>
    </view>
    <view class="password item_input flex_start">
        <input class="f_26_999 input" password placeholder='请输入密码' bindinput="onPwd"></input>
    </view>

    <view class="sub_btn login_btn_1">确认登录
      <button  bindgetuserinfo="onGetUserInfo" open-type='getUserInfo' class="lb"></button>
    </view>
    </form>
</view>
