.startMatch_box .notice_body .notice_content_box_start  .user_con
{
 display: flex;
 justify-content: center;
 flex-direction: column;
 align-items: center;
}
.startMatch_box .notice_body .notice_content_box_start  .user_name
{
  color:#ce2c39;
  font-size: 24rpx;
  text-align: center;
  margin-top: 23rpx;
  font-weight: bold;
}
.startMatch_box .notice_body .notice_content_box_start  .img_huang_guan
{
  margin-top: 30rpx;
  position: relative;
}
.startMatch_box .notice_body .notice_content_box_start  .img_huang_guan .king_bg
{
  position: absolute;
  top:0;
  left:50%;
margin-left:-100rpx;

}
.startMatch_box .notice_body .notice_content_box_start  .start_line
{
  width: 473rpx;
  margin:0 auto;
  color:white;
  font-size: 36rpx;
  position: relative;
  margin-top:30rpx;
}
.startMatch_box .notice_body .notice_content_box_start  .start_line .text
{
  position: absolute;
  left:0;
  right:0;
  top:0;
  bottom: 0;
  text-align: center;
  line-height: 73rpx;
}