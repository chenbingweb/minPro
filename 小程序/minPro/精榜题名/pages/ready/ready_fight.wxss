.ready_tip
{
  opacity: 0;
  transform:scale(3);  
  transition: all 0.3s
}
/* 动画 */
.ready_tip_animation{
  
  animation:readyTip 0.8s linear infinite;
  animation-delay: 0s;

}
@keyframes readyTip {
  0% {
   transform: rotate(0)
  }
  100%{
   transform: rotate(360deg)
  }
}

.show_Ok{
  
  animation:showOk 0.6s linear;
  animation-fill-mode:forwards;
  animation-delay: 0ms;
}
@keyframes showOk {
  0% {
    opacity: 0;
    transform:  scale(3);  
  }
  100%{
    opacity: 1;
    transform:  scale(1);  
  }
}
