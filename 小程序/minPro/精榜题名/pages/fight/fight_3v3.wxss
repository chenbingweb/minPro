.fight_page .top_bg
{
  position: absolute;
  left: 0;
  top:0;
  
}
.fight_page  .left_3v3
{
  position: absolute;
  left: 0;
  top:calc(50vh - 509rpx);
}
.fight_page .right_3v3
{
  position: absolute;
  right: 0;
  top:calc(50vh - 509rpx);
}
.fight_page .time_box
{
  position: absolute;
  width: 750rpx;
  height: 92rpx;
  left: 50%;
  top:calc(50vh - 156rpx);
  transform: translateX(-50%);
}
.fight_page .question
{
  position: absolute;
  width: 600rpx;
  height: 180rpx;
  left: 50%;
  top:calc(50vh - 80rpx);
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  justify-content: center;
  color:#333333;
  font-size: 28rpx;
  font-weight:  800;
}
.fight_page .answer_box
{
  position: absolute;
  width: 600rpx;
  height: 530rpx;
  left: 50%;
  top:calc(50vh + 80rpx);
  transform: translateX(-50%);
  color:#fff;
  font-size: 32rpx;
  font-weight:  800;
  
}
.fight_page .q_box
{
  width: 570rpx;
  height: 110rpx;
  margin: 0 auto;
  position: relative;
  margin-bottom: 20rpx;
  transform: translateX(-800rpx);
}
.fight_page .enter
{
  
  transform: translateX(0);
}
.fight_page .outer
{
 
  transform: translateX(800rpx);
}
.fight_page .q_bg
{
   width: 570rpx;
  height: 110rpx;
  background-color: #d7bd9e;
  border-radius: 55rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top:10rpx;
}
.fight_page .q_text_box
{
   width: 570rpx;
  height: 110rpx;
  background-color: #fff;
  border-radius: 55rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top:0rpx;
  color:#333333;
  font-size: 28rpx;
  text-align: center;
  line-height: 110rpx;
  padding-left:35rpx;
  padding-right:35rpx;
  box-sizing:border-box;

}
.fight_page .answer_text
{
  width: 320rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
   position: absolute;
  left: 50%;
  top:50%;
  transform: translateX(-50%) translateY(-50%);
}
.fight_page .select_yes
{
   background-color: #65ddbf;
   color:white;
}
.fight_page .select_no{
  background-color: #ee7869;
  color:white;
}
.fight_page .show_lun
{
  opacity:0;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top:calc(50vh - 100rpx);
 
}
.fight_page .lun
{
  position: absolute;
  left: 54%;
  width:200rpx;
  transform:translateX(-50%);
  top:-54rpx;
/* 
  text-align: center; */
  line-height: 221rpx;
  font-size: 140rpx;
  font-weight: bold;
  background: repeating-linear-gradient(#ffff00, #fff 50%, #ffff00);
  -webkit-background-clip: text;
  color: transparent;
  -webkit-text-stroke: 6rpx #670c09;
 
  height:230rpx;
}
.fight_page .lun_bg
{
  position:absolute;
  left: 54%;
  width:200rpx;
  transform:translateX(-50%);
  top:-54rpx;
  /* text-align:center; */
  line-height:232rpx;
  font-size:146rpx;
  font-weight:bold;
  -webkit-text-stroke:6rpx #670c09;
  color:#880101;

  height:230rpx;

}
.fight_page .double_fen
{
  width: 277rpx;
  height: 40rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top:calc(50vh - 266rpx);
  background-color: #771610;
  text-align: center;
  line-height: 40rpx;
  color:white;
  font-size: 24rpx;
  border-radius: 20rpx;
  font-weight: bold;
}
.fight_page .flex_start
{
  display:flex;
  align-items: center;
  justify-content: flex-start;
}
.fight_page .absolute
{
  position:absolute;
  left: 0;
  top:0;
  right: 0;
  bottom: 0;
}
.fight_page .relative
{
  position: relative;
}
.fight_page .icon_box
{
  display: flex;
  justify-content: center;
  align-items: center;
}
.fight_page .flex_end
{
  display:flex;
  align-items: center;
  justify-content: flex-end;
}
.fight_page .flex_center
{
  display: flex;
  align-items: center;
  justify-content: center
}
.fight_page .flex_between
{
  display: flex;
  align-items: center;
  justify-content: space-between
}
.fight_page .flex_around
{
  display: flex;
  align-items: center;
  justify-content: space-around
}
.fight_page .back
{
  position: absolute;
  left: 40rpx;
  top:calc(50vh - 613rpx);
}
.fight_page .btn_login,.bl
{
  position: absolute;
  top:0;
  left: 0;
  right:0;
  bottom:0;
  opacity: 0;
}