/*左侧连对进入*/
.friend_fight .leftld_in
{
  animation:leftld_inKeys 2s linear;
  animation-fill-mode:forwards; 
}

@keyframes leftld_inKeys {
  0% {
    transform: translate(-200rpx);
  }

  10%{
    transform: translate(0rpx);
  } 

  90% { 
    transform: translate(0rpx) scale(1); 
    opacity: 1;
  }
  100%{ 
    transform: translate(40rpx) scale(1.4);
    opacity: 0;
  } 
}

/*左侧连队消失*/
.friend_fight .leftld_out
{
  animation:leftld_outKeys 0.3s linear;
  animation-fill-mode:forwards; 
}
 
@keyframes leftld_outKeys {
  0% { 
    transform: translate(0rpx) scale(1); 
    opacity: 1;
  }
  100%{ 
    transform: translate(40rpx) scale(1.4);
    opacity: 0;
  } 
}


/*右侧连对进入*/
.friend_fight .rightld_in
{
  animation:rightld_inKeys 2s linear;
  animation-fill-mode:forwards; 
}

@keyframes rightld_inKeys {
  0% {
    transform: translate(200rpx);
  }
  10%{
    transform: translate(0rpx);
  } 

  90% {
    transform: translate(0rpx) scale(1);
     opacity: 1;
  }
  100%{
    transform: translate(-40rpx) scale(1.4);
     opacity: 0;
  } 
}

/*右侧连对淡出*/
.friend_fight .rightld_out
{
  animation:rightld_outKeys 0.3s linear;
  animation-fill-mode:forwards; 
}

@keyframes rightld_outKeys {
  0% {
    transform: translate(0rpx) scale(1);
     opacity: 1;
  }
  100%{
    transform: translate(-40rpx) scale(1.4);
     opacity: 0;
  } 
}

 



 
