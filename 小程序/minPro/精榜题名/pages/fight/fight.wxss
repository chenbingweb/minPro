  position: absolute;
  top: -50rpx;
  left: 20rpx;
}

/*3V3  */

.friend_fight .group_bg {
  position: absolute;
  left: 0;
  top: 0;
}

.friend_fight  .group_bg_right {
  position: absolute;
  right: 0;
  top: 0;
  transform: rotateY(180deg);
}

/*沉默状态*/

.friend_fight .cm_status {
  position: absolute;
  top: 0;
  left: 0;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.7);
  text-align: center;
  line-height: 100rpx;
  font-size: 25rpx;
  color: #f00;
  font-weight: bold;
}

.friend_fight .jshu_box {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  display: inline-flex;
  justify-content: center;
  align-items: center;
}

.friend_fight .jshu_box .show_time {
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  background-color: #fff;
  line-height: 66rpx;
  overflow: hidden;
}

.friend_fight .chen_mo_tip {
  width: 122rpx;
  height: 59rpx;
  margin: 0 auto;
  position: absolute;
  top: -30rpx;
  left: 50%;
  margin-left: -61rpx;
  box-sizing: border-box;
}

.friend_fight .question_box .time.an {
  animation: SCale 1s infinite;
}

@keyframes SCale {
  0% {
    transform: scale(1);
  }

  10% {
    transform: scale(1.2);
  }

  100% {
    transform: scale(1);
  }
}


.friend_fight .speak_img{

  font-size: 22rpx;
  /* position: absolute; */
  margin-left:  5rpx;
  margin-top:  10rpx; 
}