@import './fightOver.wxss';
@import './fightOver_1.wxss';
page
{
  width: 100%;
  height: 100%;
  background-color: #f6dcba;
 /* background-image:linear-gradient(0deg,#0d3d5b,#4f2e73);  */
}
.fightOver .share_btn_an
{
  animation:Anima_1 0.3s linear;
  animation-fill-mode:forwards;
  animation-delay: 2000ms;
}

 @keyframes Anima_1 {
    0% {
      left:-200rpx;
    
    }
    100%
    {
      left: 0;
    }
  }

.fightOver .Ani_Fight_show
{
  animation:Ani_Fight 0.3s linear;
  animation-fill-mode:forwards;
  animation-delay: 0s
}
@keyframes Ani_Fight {
  0% {
    opacity: 0
  }
  50%{
   opacity: 0.5
  }
  100%
  {
    opacity: 1
  }
}