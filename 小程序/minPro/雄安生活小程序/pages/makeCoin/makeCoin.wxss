/* pages/makeCoin/makeCoin.wxss */
.header_box
{
  height: 211rpx;
  width: 750rpx;

}
.account_title
{
  color:#fff;
  font-size: 28rpx;
  text-align: center;
  margin-top: 45rpx;
}
.account_coin
{
  color:#fff;
  font-size: 50rpx;
  text-align: center;
}

.item_box
{
  width: 750rpx;
  margin: 0 auto;
  height: 126rpx;
  box-sizing: border-box;
  padding: 0 25rpx;
  border-bottom: 2rpx solid #e3e3e3;
}
.info_share
{
  font-size: 30rpx;
  color:#333;
  padding-left: 20rpx;
}
.item_share_btn
{
  width: 150rpx;
  height: 58rpx;
  border-radius: 29rpx;
  background-color: #fed545;
  text-align: center;
  line-height: 58rpx;
  font-size: 30rpx;
  color:#ffffff;
  box-sizing: border-box;
}
.do
{
  border:2rpx solid #f4f4f4;
  background-color: white;
  color:#333333;
  font-size: 30rpx;
}