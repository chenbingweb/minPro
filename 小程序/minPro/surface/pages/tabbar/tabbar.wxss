.tabbar_box
{
  width: 100%;
  height: 100rpx;
  background-color:white;
  position: fixed;
  z-index: 9999;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  box-sizing: border-box;
  align-items: center;
}
.bar_imt_box
{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 40rpx;
}
.tabbar_item
{
  width: 20%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  color:#666;
  height: 64rpx;
  box-sizing: border-box;
}
.tabbar_home
{
  width: 34rpx;
  height: 30rpx;
}