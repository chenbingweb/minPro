/* signIn.wxss */
.sign_in_bg
{
  background-color: #30B5F2 ;
  background-repeat: no-repeat;
  background-position: 0 0;
  background-size: 100% 100%;
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.sign_title
{
  width: 516rpx;
  height: 90rpx;
  color:#fff;
  font-size: 36rpx;
  text-align: center;
  line-height: 78rpx;
  background-image: url("data:image/png;base64,");
  background-position: 0 0;
  background-size: 100% 100%;
  margin:0 auto;
  margin-top:10.945vh;
}
.sign_box
{
  width: 516rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.today
{
  color:#333;
  font-size: 30rpx;
  margin:1.66vh 0;
}
.get_score>image
{
  width:28rpx;
  height: 22rpx;
}
.days_box
{
  color:#999999;
  font-size: 36rpx;
  margin-bottom:1.66vh;
  line-height:36rpx;
}
.days
{
  color:#f25022;
  font-size: 48rpx;
  font-weight: bold;
  margin:0 60rpx;
}
.get_score
{
  display: flex;
  justify-content: center;
  align-items:center;
  height:30rpx;

}
.get_score_box
{
  display: inline-flex;
  justify-content: baseline;
  align-items: flex-start;
  
}
.get_score_box>text
{
  color:#f25022;
  font-size:30rpx;
  padding-right:6rpx;
  line-height:30rpx;

}
/*每日签到**/
.sign_container
{
  width: 690rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding:0 64rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top:3.64vh;
  /*
  margin-top:40rpx;*/
}

.sign_day
{
  
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  /*width: 106rpx;*/
}
.sign_day_circle
{
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
  background-color: #F3592F;
  text-align: center;
  line-height: 82rpx;
  color:#fff;
  font-size: 26rpx;
}
.no_sign
{
  background-color: #ccc;
}
.sign_day_text
{
  color:#666666;
  font-size: 26rpx;
  line-height:26rpx;

}
/**w我的积分*/
.my_score_total
{
  color:#f25022;
  font-size: 30rpx;
  width: 600rpx;
  margin: 0 auto;
  text-align: center;
   margin-top:4.146vh;
  line-height:30rpx;

   /*
  margin-top:35rpx;*/
}
/**签到*/
.submit_box
{
   width: 690rpx;
  height: 82rpx;
  box-sizing: border-box;
  margin:0 auto;
 margin-top:9.95vh;
}
.submit_sign
{
  width: 690rpx;
  height: 82rpx;
  box-sizing: border-box;
  background-color: #FBB900;
  color:#875c0d;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  line-height: 82rpx;
  margin: 0 auto;
  border-top-left-radius: 41rpx;
  border-top-right-radius: 41rpx;
  border-bottom-left-radius: 41rpx;
  border-bottom-right-radius: 41rpx;
  box-shadow: 0 4rpx 6rpx 2rpx #5CACB1;
}
/*规则*/
.sign_rule
{
  width: 100%;
  text-align: center;
  box-sizing: border-box;
  justify-content: space-between;
  margin-top:1.66vh;
}
.rules
{
  color:#ffffff;
  font-size: 36rpx;
  line-height: 36rpx;
  display: inline-flex;
  align-items: center;
}
.rules::before,.rules::after
{
  content: '';
  display: inline-block;
  width:246rpx;
  border-top:2rpx solid white;
}
.rules::after
{
  margin-left:16rpx;
}
.rules::before
{
  margin-right:16rpx;
}
.sign_content
{
  color:#fff;
  font-size:26rpx;
  width: 100%;
  box-sizing: border-box;
  padding-left: 40rpx;
   margin-top:3.3167vh;
 
}
.sign_content view
{
  margin-bottom: 20rpx;
}